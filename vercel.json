{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true
    }
  },
  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/tools/print-schedule",
      "destination": "https://schedule.randombits.dev",
      "permanent": true
    }
  ],
  "trailingSlash": false
}
