---
import * as Terms from "../../../snippits/font-dash/terms.md";
import BaseLayout from "../../../layouts/BaseLayout.astro";
import {Image} from 'astro:assets';
import fontDashLogo from '@/assets/canva/font-dash/logo.png';
---
<BaseLayout title="FontDash Terms and Conditions">
  <div class="container-sm py-24 snippit">
    <div class="flex items-center">
      <Image width="100" src={fontDashLogo} alt="FontDash Logo" class="border-solid border-2 rounded-lg mr-10"/>
      <h1>FontDash Terms and Conditions</h1>
    </div>
    <Terms.Content></Terms.Content>
  </div>
</BaseLayout>
