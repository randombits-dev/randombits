{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ]
    }
  }
}